{
  "name": "education-platform-frontend",
  "version": "1.0.0",
  "description": "AWS Education Platform Frontend - React SPA for online education",
  "private": true,
  "homepage": "/",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "react-scripts": "5.0.1",
    "axios": "^1.3.0",
    "aws-amplify": "^5.0.0",
    "@aws-amplify/ui-react": "^4.0.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.0.0",
    "@aws-sdk/client-s3": "^3.0.0",
    "styled-components": "^5.3.0",
    "react-helmet": "^6.1.0",
    "@heroicons/react": "^2.0.0",
    "date-fns": "^2.29.0",
    "web-vitals": "^3.1.0",
    "@apollo/client": "^3.7.0",
    "graphql": "^16.6.0",
    "react-player": "^2.12.0",
    "qrcode.react": "^3.1.0",
    "react-qr-scanner": "^1.0.0-alpha.11",
    "recharts": "^2.5.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "eslint": "^8.36.0",
    "eslint-plugin-react": "^7.32.0",
    "prettier": "^2.8.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --write src/**/*.{js,jsx,css,md}",
    "lint": "eslint src/**/*.{js,jsx}",
    "predeploy": "npm run build",
    "deploy": "aws s3 sync build/ s3://$S3_BUCKET_NAME --delete",
    "invalidate": "aws cloudfront create-invalidation --distribution-id $CLOUDFRONT_ID --paths '/*'"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "proxy": "http://localhost:3001"
}
